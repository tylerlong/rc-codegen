{
  "name": "rc-codegen",
  "version": "0.1.0",
  "description": "RingCentral CodeGen",
  "main": "index.js",
  "scripts": {
    "upgrade": "yarn-upgrade-all",
    "compile": "tsc -w",
    "swift": "node index.js -l swift -o ~/src/swift/ringcentral-swift/Source -t ~/src/ts/rc-codegen/swift/views",
    "csharp": "node index.js -l csharp -o ~/Projects/RingCentral/RingCentral -t ~/src/ts/rc-codegen/csharp/views",
    "ruby": "node index.js -l ruby -o ~/src/ruby/ringcentral-ruby/src -t ~/src/ts/rc-codegen/ruby/views",
    "java": "node index.js -l java -o ~/src/java/ringcentral-java/src/main/java/com/ringcentral -t ~/src/ts/rc-codegen/java/views"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylerlong/rc-codegen.git"
  },
  "keywords": [
    "codegen",
    "swagger",
    "RingCentral"
  ],
  "author": "Tyler Long <tyler4long@gmail.com> (https://github.com/tylerlong)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tylerlong/rc-codegen/issues"
  },
  "homepage": "https://github.com/tylerlong/rc-codegen#readme",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/lodash": "^4.14.101",
    "@types/node": "^9.4.1",
    "@types/nunjucks": "^3.0.0",
    "typescript": "^2.7.1",
    "yarn-upgrade-all": "^0.3.0"
  },
  "dependencies": {
    "commander": "^2.14.0",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.19",
    "nunjucks": "^3.0.1"
  }
}
